install.packages("astsa")
install.packages("astsa")
x <- scan("quakes.dat")
x <- ts(x)
plot(x, type="b")
lag1.plot(x,1)
acf(x,xlim=c(1,19))
xlag1 <- lag(x,-1)
y <- cbind(x,xlag1)
ar1fit <- lm(y[,1]~y[,2])
summary(ar1fit)
plot(ar1fit$fit,ar1fit$residuals)
acf(ar1fit$residuals,xlim=c(1,18))
